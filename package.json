    "lint:fix": "tslint \"src/**/*.{ts,tsx}\" --project ./tsconfig.json --fix",
    "lint:watch": "yarn lint -- --watch"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "1.6.5",
    "jwt-decode": "^2.2.0",
    "react": "16.13.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "tslint": "^6.1.1",
    "tslint-react": "^4.2.0"
  }
